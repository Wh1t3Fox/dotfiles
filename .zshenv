export GDK_SCALE=1
export GDK_DPI_SCALE=1
export QT_AUTO_SCREEN_SCALE_FACTOR=1

export TERM='xterm-256color'
export EDITOR='vim'
export GOPATH=~/go
export XDG_CONFIG_HOME="$HOME/.config"
export PATH="$HOME/bin:$HOME/.local/bin:$GOPATH/bin:$HOME/node_modules/.bin:$PATH:$HOME/.cargo/bin:$HOME/.virtualenvs/tempor/bin"
export KUBECONFIG="$HOME/.kube/config"
export SSH_AUTH_SOCK=~/.1password/agent.sock
export PAGER=
export TINTED_SHELL_ENABLE_VARS=1
export FZF_DEFAULT_OPTS="$FZF_DEFAULT_OPTS"\
" --color=bg:0,fg:7,hl:3"\
" --color=bg+:8,fg+:7,hl+:11"\
" --color=info:3,border:3,prompt:4"\
" --color=pointer:0,marker:9,spinner:9,header:1"
